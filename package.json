{
  "name": "naylence-advanced-security",
  "version": "0.1.0",
  "type": "module",
  "description": "Advanced security utilities for the Naylence Fame runtime implemented in TypeScript.",
  "author": "Naylence Dev <naylencedev@gmail.com>",
  "license": "Apache-2.0",
  "keywords": [
    "naylence",
    "fame",
    "security",
    "agents",
    "encryption",
    "certificates",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/naylence/naylence-advanced-security-ts"
  },
  "bugs": {
    "url": "https://github.com/naylence/naylence-advanced-security-ts/issues"
  },
  "homepage": "https://github.com/naylence/naylence-advanced-security-ts#readme",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./browser": {
      "import": "./dist/browser/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./plugin": {
      "import": "./dist/esm/plugin.js",
      "default": "./dist/esm/plugin.js",
      "types": "./dist/types/plugin.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "NOTICE"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:types && npm run build:cjs && npm run build:esm && npm run build:browser",
    "build:types": "tsc --emitDeclarationOnly --outDir dist/types",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:browser": "rollup -c rollup.config.js",
    "clean": "rimraf dist",
    "generate:factory-manifest": "node ./scripts/generate-factory-manifest.mjs",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --passWithNoTests",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "dev": "tsc --watch --outDir dist/esm",
    "prebuild": "npm run generate:factory-manifest",
    "predev": "npm run generate:factory-manifest",
    "prepublishOnly": "npm run build && npm test"
  },
  "dependencies": {
    "@noble/ciphers": "^2.0.1",
    "@noble/curves": "^1.4.0",
    "@noble/hashes": "^1.4.0",
    "@peculiar/x509": "^1.14.0",
    "jose": "^6.1.0",
    "naylence-core": "file:../naylence-core-ts",
    "naylence-factory": "file:../naylence-factory-ts",
    "naylence-runtime": "file:../naylence-runtime-ts",
    "yaml": "^2.6.0",
    "zod": "^4.1.11"
  },
  "peerDependencies": {
    "ws": "^8.0.0"
  },
  "peerDependenciesMeta": {
    "ws": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.205.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-trace-base": "^2.1.0",
    "@opentelemetry/sdk-trace-node": "^2.1.0",
    "better-sqlite3": "^12.4.1",
    "pino": "^9.12.0",
    "pino-pretty": "^13.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.6.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "jest-util": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.52.3",
    "ts-jest": "^29.4.4",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2",
    "ws": "^8.18.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
